{
  "name": "static",
  "version": "1.1.0",
  "description": "for static build",
  "main": "start.html",
  "author": "daisuke85a",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean:public": "rimraf testdir",
    "clean": "rimraf testdir .cache",
    "dev": "parcel src/view/*.pug --open --out-dir temp",
    "build": "parcel build src/view/*.pug --no-source-maps --no-cache -d testdir --public-url ./",
    "prebuild": "yarn clean:public",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines.browsers": [
    "defaults",
    "not < 1%"
  ],
  "devDependencies": {
    "node-sass": "^4.11.0",
    "parcel-bundler": "^1.11.0",
    "pug": "^2.0.3",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "destyle.css": "^1.0.5",
    "yakuhanjp": "^3.0.0"
  }
}
